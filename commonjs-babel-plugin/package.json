	},
	"scripts": {
		"all": "npm run spellcheck && npm run lint && npm run test",
		"lint": "npx eslint .",
		"spellcheck": "find -type f \\( -name \"*.js\" -or -name \"*.md\" \\) ! -path \"./node_modules/**/*\" ! -path \"./tests/output/**/*\" -exec npx cspell -c ../cspell.json {} +",
